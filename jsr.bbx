%%% 著者名のフォーマット
%%% 日本語文献と英語文献で処理を変える
%%% 著者名が20名を超える場合は途中を省略
\DeclareNameFormat{default}{%
  \ifthenelse{\boolean{japanese}}{%
    % 著者総数が\maxprtnamesより大きく，かつ著者の順番が\maxprtnamesと同じ場合
    \ifthenelse{\value{listcount}=\maxprtnames\AND\value{listcount}<\value{listtotal}}{%
      % 「...」で省略
      \space\ldots\space%
      }{%
        % 処理中の著者の順番が\maxprtnamesより大きく，総数未満である場合
        \ifthenelse{\value{listcount}>\maxprtnames\AND\value{listcount}<\value{listtotal}}{%
        }{%
        \usebibmacro{name:family-given}%
          {\namepartfamily}%
          {\namepartgiven}%
          {\namepartprefix}%
          {\namepartsuffix}%
      }%
    }%
  }{% 英語文献の場合、最初の名前のみ姓・名でそのほかは名・姓とする
    \ifthenelse{\value{listcount}=\maxprtnames\AND\value{listcount}<\value{listtotal}}
      {\addcomma\space\ldots\space}
      {\ifnumequal{\value{listcount}}{1}%
      {\ifgiveninits%
         {\usebibmacro{name:family-given}%
           {\namepartfamily}%
           {\namepartgiveni}%
           {\namepartprefix}%
           {\namepartsuffix}}%
         {\usebibmacro{name:family-given}%
           {\namepartfamily}%
           {\namepartgiven}%
           {\namepartprefix}%
           {\namepartsuffix}}%
       \ifboolexpe{%
         test {\ifdefvoid\namepartgiven}
         and
         test {\ifdefvoid\namepartprefix}}%
         {}%
         {\usebibmacro{name:revsdelim}}}%
      {\ifgiveninits%
         {\usebibmacro{name:given-family}%
           {\namepartfamily}%
           {\namepartgiveni}%
           {\namepartprefix}%
           {\namepartsuffix}}%
         {\usebibmacro{name:given-family}%
           {\namepartfamily}%
           {\namepartgiven}%
           {\namepartprefix}%
           {\namepartsuffix}}}%
    }%
  }%
}
%%% ファーストネーム（イニシャル），ファミリーネームの順（reprintなど文献リストの先頭ではない部分に名前が登場する場合に備えて）
\DeclareNameFormat{j:family-givem/e:given-family}%
{\ifthenelse{\boolean{japanese}}%
  {\ifthenelse{\value{listcount}=\maxprtnames\AND\value{listcount}<\value{listtotal}}%
    {\space\ldots\space}%
    {\ifthenelse{\value{listcount}>\maxprtnames\AND\value{listcount}<\value{listtotal}}%
      {}
      {\usebibmacro{name:family-given}%
        {\namepartfamily}%
        {\namepartgiven}%
        {\namepartprefix}%
        {\namepartsuffix}}}}%
  {\ifthenelse{\value{listcount}=\maxprtnames\AND\value{listcount}<\value{listtotal}}%
    {\addcomma\space\ldots\space}%
    {\ifthenelse{\value{listcount}>\maxprtnames\AND\value{listcount}<\value{listtotal}}%
      {}
      {\ifgiveninits%
        {\usebibmacro{name:given-family}%
          {\namepartfamily}%
          {\namepartgiveni}%
          {\namepartprefix}%
          {\namepartsuffix}}%
        {\usebibmacro{name:given-family}%
          {\namepartfamily}%
          {\namepartgiven}%
          {\namepartprefix}%
          {\namepartsuffix}}}}}}

