%%% 著者名のフォーマット
%%% 日本語文献と英語文献で処理を変える
%%% 著者名が20名を超える場合は途中を省略
\DeclareNameFormat{default}{%
  \ifthenelse{\boolean{japanese}}{%
    % 著者総数が\maxprtnamesより大きく，かつ著者の順番が\maxprtnamesと同じ場合
    \ifthenelse{\value{listcount}=\maxprtnames\AND\value{listcount}<\value{listtotal}}{%
      % 「...」で省略
      \space\ldots\space%
      }{%
        % 処理中の著者の順番が\maxprtnamesより大きく，総数未満である場合
        \ifthenelse{\value{listcount}>\maxprtnames\AND\value{listcount}<\value{listtotal}}{%
        }{%
        \usebibmacro{jpname:family-given}%
          {\namepartfamily}%
          {\namepartgiven}%
          {\namepartprefix}%
          {\namepartsuffix}%
      }%
    }%
  }{% 英語文献の場合、最初の名前のみ姓・名でそのほかは名・姓とする
    \ifthenelse{\value{listcount}=\maxprtnames\AND\value{listcount}<\value{listtotal}}
      {\addcomma\space\ldots\space}
      {\ifnumequal{\value{listcount}}{1}%
      {\ifgiveninits%
         {\usebibmacro{name:family-given}%
           {\namepartfamily}%
           {\namepartgiveni}%
           {\namepartprefix}%
           {\namepartsuffix}}%
         {\usebibmacro{name:family-given}%
           {\namepartfamily}%
           {\namepartgiven}%
           {\namepartprefix}%
           {\namepartsuffix}}%
       \ifboolexpe{%
         test {\ifdefvoid\namepartgiven}
         and
         test {\ifdefvoid\namepartprefix}}%
         {}%
         {\usebibmacro{name:revsdelim}}}%
      {\ifgiveninits%
         {\usebibmacro{name:given-family}%
           {\namepartfamily}%
           {\namepartgiveni}%
           {\namepartprefix}%
           {\namepartsuffix}}%
         {\usebibmacro{name:given-family}%
           {\namepartfamily}%
           {\namepartgiven}%
           {\namepartprefix}%
           {\namepartsuffix}}}%
    }%
  }%
}
\DeclareNameAlias{author}{default}
\DeclareNameAlias{editor}{default}
\DeclareNameAlias{editora}{default}
\DeclareNameAlias{translator}{default}
\DeclareNameAlias{translatora}{default}
\DeclareNameAlias{origauthor}{default}
\DeclareNameAlias{bookauthor}{default}

%%% ファーストネーム（イニシャル），ファミリーネームの順
\DeclareNameFormat{given-family}{%
  \ifthenelse{\boolean{japanese}}{%
    \ifthenelse{\value{listcount}=\maxprtnames\AND\value{listcount}<\value{listtotal}}{%
      \space\ldots\space%
      }{%
        \ifthenelse{\value{listcount}>\maxprtnames\AND\value{listcount}<\value{listtotal}}{}{%
        \usebibmacro{jpname:family-given}%
          {\namepartfamily}%
          {\namepartgiven}%
          {\namepartprefix}%
          {\namepartsuffix}%
      }%
    }%
  }{%
    \ifthenelse{\value{listcount}=\maxprtnames\AND\value{listcount}<\value{listtotal}}{%
        \addcomma\space\ldots\space%
      }{%
        \ifthenelse{\value{listcount}>\maxprtnames\AND\value{listcount}<\value{listtotal}}{}{%
        \ifgiveninits%
        {\usebibmacro{name:given-family}%
          {\namepartfamily}%
          {\namepartgiveni}%
          {\namepartprefix}%
          {\namepartsuffix}}%
        {\usebibmacro{name:given-family}%
          {\namepartfamily}%
          {\namepartgiven}%
          {\namepartprefix}%
          {\namepartsuffix}}%
        }%
    }%
  }%
}
%%% 日本著者の表示設定（英語著者で設定される姓名間のスペースの削除）
\newbibmacro*{jpname:family-given}[4]{%
  \ifuseprefix
    {\usebibmacro{name:delim}{#3#1}%
     \usebibmacro{name:hook}{#3#1}%
     \mkbibcompletenamefamilygiven{%
       \ifdefvoid{#3}
         {}
         {\ifcapital
            {\mkbibnameprefix{\MakeCapital{#3}}\isdot}
            {\mkbibnameprefix{#3}\isdot}%
          \ifprefchar{}{\bibnamedelimc}}%
       \mkbibnamefamily{#1}\isdot
       \ifdefvoid{#4}
         {}
         {\mkbibnamesuffix{#4}\isdot}%
       \ifdefvoid{#2}
         {}
         {\revsdnamepunct\mkbibnamegiven{#2}\isdot}}}
    {\usebibmacro{name:delim}{#1}%
     \usebibmacro{name:hook}{#1}%
     \mkbibcompletenamefamilygiven{%
       \mkbibnamefamily{#1}\isdot
       \ifdefvoid{#4}
         {}
         {\mkbibnamesuffix{#4}\isdot}%
       \ifboolexpe{%
         test {\ifdefvoid{#2}}
         and
         test {\ifdefvoid{#3}}}
         {}
         {\revsdnamepunct}%
       \ifdefvoid{#2}
         {}
         {\mkbibnamegiven{#2}\isdot}%
       \ifdefvoid{#3}
         {}
         {\mkbibnameprefix{#3}\isdot}}}}


