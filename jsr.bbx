    {\printnames[given-family]{editor}%
    \setunit{\printdelim{editortypedelim}}%
    \usebibmacro{ed-eds:editor}%
    \setunit{\printdelim{titleafterdelim}}}%
  \usebibmacro{byeditorx}}

